gset <- gset[[idx]]
# make proper column names to match toptable
fvarLabels(gset) <- make.names(fvarLabels(gset))
fix(gset)
gset
eset <- exprs(gset)
View(eset)
write.table(eset, "GSE37900forGSEA.txt", sep="\t", quote=FALSE)
